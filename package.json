{
  "name": "clean-text-svelte",
  "version": "0.1.0",
  "description": "Clean text to prepare for pasting into a document",
  "repository": "github:theodore-s-beers/clean-text-svelte",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "standardize": "standard --fix ./rollup.config.js",
    "standardize-ts": "ts-standard --fix ./src/main.ts",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "dependencies": {
    "sirv-cli": "^1.0.10"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.0",
    "rollup": "^2.38.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^16.0.3",
    "svelte": "^3.32.0",
    "svelte-check": "^1.1.28",
    "svelte-preprocess": "^4.6.5",
    "ts-standard": "^10.0.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
}
